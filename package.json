{
  "name": "redux-orm",
  "version": "0.10.0",
  "description": "Simple ORM to manage and query your state trees",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "prepare": "npm run build",
    "build": "npm run clean && babel src --out-dir lib && webpack --config webpack.dev.js && webpack --config webpack.prod.js && jsdoc src -d docs -c ./jsdoc.conf.json --verbose",
    "pub": "npm run lint && npm run test && npm run build && npm version patch && npm publish && git push origin && gulp deploy && npm run clean",
    "lint": "eslint src",
    "deploydocs": "npm run build && gulp deploy",
    "clean": "rimraf lib dist docs"
  },
  "keywords": [
    "redux",
    "orm"
  ],
  "author": "Tommi Kaikkonen <tommi.kaikkonen@aalto.fi>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommikaikkonen/redux-orm.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-classes": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-2": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "codecov": "^3.0.2",
    "deep-freeze": "0.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "gulp": "^4.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-rename": "^1.2.2",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jsdoc": "^3.5.5",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "immutable-ops": "^0.5.2",
    "lodash": "^4.17.10",
    "reselect": "^2.5.4"
  }
}
